from flask import Flask, request, render_template
import yfinance as yf
app = Flask(__name__)
from flask import Flask, request, render_template
import yfinance as yf

app = Flask(____name____)

@app.route("/quote")
def display_quote():
  symbol = request.args.get('symbol', default="AAPL")

  quote = yf.Ticker(symbol)

  return quote.info

if ____name____ == "____main____":
  app.run(debug=True)
